    def result(self):
        self.button = main_window.sender()
        self.text = self.button.text()
        if self.text == "C":
                self.lineEdit.clear()
        elif self.text == "=":
            try:
                self.resulf = eval(self.label.text)
                self.lineEdit.setText(str(self.resulf))
            except:
                self.lineEdit.setText("Помилка")
        else:
             self.lineEdit.setText(self.label.text() + self.text)

app = QtWidgets.QApplication([])
main_window = Main()
main_window.show()
app.exec()
